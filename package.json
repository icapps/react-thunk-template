{
  "name": "react_thunk_template",
  "description": "React Thunk Template",
  "version": "0.1.0",
  "dependencies": {
    "axios": "~0.18.0",
    "core-js": "2.5.7",
    "node-sass-chokidar": "~0.0.3",
    "npm-run-all": "~4.1.2",
    "prop-types": "~15.6.0",
    "react": "~16.2.0",
    "react-app-polyfill": "^0.1.3",
    "react-dom": "~16.2.0",
    "react-redux": "~5.0.6",
    "react-router-dom": "~4.2.2",
    "react-scripts": "2.0.5",
    "redux": "~3.7.2",
    "redux-logger": "~3.0.6",
    "redux-thunk": "~2.2.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build && rm build/static/**/*.map",
    "test": "react-scripts test",
    "coverage": "npm test -- --coverage --collectCoverageFrom='[\"src/**/*.{js,jsx}\",\"!src/index.js\",\"!src/utils/index.js\",\"!src/registerServiceWorker.js\",\"!src/components/index.js\"]'",
    "lint": "./node_modules/.bin/eslint -c .eslintrc --ext .jsx --ext .js src",
    "validate": "yarn lint && yarn test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-eslint": "9.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "5.6.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.33.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.4.0",
    "react-test-renderer": "^16.2.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
